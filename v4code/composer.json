{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bacon/bacon-qr-code": "^2.0",
        "block_io-php/block_io-php": "^3.0",
        "buzz/laravel-google-captcha": "^2.3",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "joshbrw/laravel-module-installer": "^2.0",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.1",
        "nwidart/laravel-modules": "^10.0",
        "phpmailer/phpmailer": "^6.7",
        "pragmarx/google2fa-laravel": "^2.1",
        "stripe/stripe-php": "^10.7",
        "twilio/sdk": "^6.44",
        "yajra/laravel-datatables": "^10.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.19",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/helpers/helpers.php",
            "app/helpers/files.php",
            "app/helpers/modules.php",
            "app/helpers/strings.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Infoamin\\Installer\\": "packages/infoamin/laravel-installer/src"
        },
        "files": [
            "packages/infoamin/laravel-installer/src/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "joshbrw/laravel-module-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
